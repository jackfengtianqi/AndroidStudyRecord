<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.layoutdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LayoutActivity.LinearLayoutActivity"
            android:label="@string/Linear"></activity>
        <activity
            android:name=".LayoutActivity.RelativeLayoutActivity"
            android:label="@string/Realtive"></activity>
        <activity
            android:name=".LayoutActivity.MyLayoutActivity"
            android:label="@string/My"></activity>
        <activity
            android:name=".LayoutActivity.FrameLayoutActivity"
            android:label="@string/Frame"></activity>
        <activity
            android:name=".LayoutActivity.TableLayoutActivity"
            android:label="@string/Table"></activity>
        <activity
            android:name=".LayoutActivity.AbsoluteLayoutActivity"
            android:label="@string/Absolute"></activity>
    </application>

</manifest>